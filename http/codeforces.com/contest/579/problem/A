#include <iostream>
#include <algorithm>
#include <string>
using namespace std;
long long power(long long n, long long m)
{
	long long f = 1;

	for (int i = 0; i<m; i++)
		f *= n;

	return f;
}
int main()
{
	long long  n, x, c = 0, i = 30;
	cin >> n;


	while (true)
	{
		x = power(2, i);
		if (x<=n)
		{
			n-=x;
			c++;
		}
		if (n == 0)
			break;
		i--;
	}

	cout <<c << endl;

	//system("pause");
	return 0;
}
